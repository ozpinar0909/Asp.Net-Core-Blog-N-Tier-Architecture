@using EntityLayer.Concrete
@model List<Category>

@{
    Layout = null;
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Kategori Adı</th>
        <th>Durumu</th>
        <th>Detaylar</th>
        <th>Sil</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.CategoryID</td>
            <td>@item.CategoryName</td>
            <td>@item.CategoryStatus</td>
           <a class="btn btn-warning">Detaylar</a> 
           <a class="btn btn-danger">Sil</a> 
        </tr>
    }

</table>